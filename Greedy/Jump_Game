https://leetcode.com/problems/jump-game/submissions/1860352978

class Solution {
public:
    bool canJump(vector<int>& nums) {
       int maxi=0;
       //we find out what is the maximium index the jump can touch
       for(int i=0;i<nums.size();i++)
       {
        if(i<=maxi)
        //that is the index can be touched
        {
            
            maxi=max(maxi,nums[i]+i);
        }
        else
        //the index can not be couched
        {
            return false;
        }
       }
       return true; 
    }
};
